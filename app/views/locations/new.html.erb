<h1>New City Form </h1>


<%= form_for @location do |f| %>
  <%=f.label :name %>
  <%=f.text_field :name %>

  <%=f.label :state %>
  <%= f.select(:state, options_for_select(@state_collection)) %>

  <%=f.label :description %>
  <%=f.text_area :description %>

  <%= f.submit "Add City" %>
<% end %>
